@model ProiectASP.Models.Group

<div class="card">


    <div class="card-body">

        <h3 class="card-title alert-success py-3 px-3 rounded-2">@Model.Name</h3>

        <div class="card-text">@Model.Description</div>
    </div>

    @foreach(var user in ViewBag.Users)
    {
        <div class="card-body">

            <h3 class="card-title alert-success py-3 px-3 rounded-2">@user.User.UserName</h3>

        </div>
    }

</div>